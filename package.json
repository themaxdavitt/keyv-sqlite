{
  "name": "@keyv/sqlite",
  "version": "2.0.2",
  "description": "SQLite storage adapter for Keyv",
  "main": "src/index.js",
  "scripts": {
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "xo": {
    "extends": "xo-lukechilds",
    "rules": {
      "indent": "off",
      "padding-line-between-statements": "off",
      "unicorn/numeric-separators-style": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prevent-abbreviations": "off"
    }
  },
  "ava": {
    "require": [
      "esm",
      "requirable"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukechilds/keyv-sqlite.git"
  },
  "keywords": [
    "sqlite",
    "sql",
    "keyv",
    "storage",
    "adapter",
    "key",
    "value",
    "store",
    "cache",
    "ttl"
  ],
  "author": "Luke Childs <lukechilds123@gmail.com> (http://lukechilds.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukechilds/keyv-sqlite/issues"
  },
  "homepage": "https://github.com/lukechilds/keyv-sqlite",
  "dependencies": {
    "@keyv/sql": "^1.1.2",
    "pify": "^3.0.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@keyv/test-suite": "*",
    "ava": "^3.15.0",
    "coveralls": "^3.1.0",
    "eslint-config-xo-lukechilds": "^1.0.1",
    "esm": "^3.2.25",
    "keyv": "*",
    "nyc": "^15.1.0",
    "requirable": "^1.0.5",
    "this": "^1.1.0",
    "xo": "^0.40.2"
  }
}
